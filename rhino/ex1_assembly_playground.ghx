                    <item name="CodeInput" type_name="gh_string" type_code="10">import Rhino.Geometry as rg
import math
from scriptcontext import sticky as st


from compas.geometry import Frame
from compas.geometry import Transformation
from compas.geometry import Rotation
from compas.geometry import Translation
from compas.geometry import Point

#from compas_ghpython.geometry import xtransformed
from compas_ghpython import draw_frame

from assembly_information_model.assembly import Assembly
from assembly_information_model.assembly import Element

import Rhino.Geometry as rg






# initialize an empty assembly class
assembly = Assembly()

planes = [] # just for vizualisation


#cp.Rotate(math.radians(90), cp.ZAxis, cp.Origin)
#cp.Translate(rg.Vector3d(0,0,1))
cp = centerplane
cf = Frame(cp.Origin, cp.XAxis, cp.YAxis)


for i in range(25): # loop through the number of layers i
    
    axis = cf.zaxis
    angle = math.radians(i*3)
    
    R = Rotation.from_axis_and_angle(axis, angle, point=cp.Origin) #Calculates a ``Rotation`` from a rotation axis and an angle and an optional point of rotation.
    T = Translation.from_vector([0,0,0.005*i])
   

    cft = cf.transformed(R * T) #transforms the origin plane

    if i%2 == 0: 
        R = Rotation.from_axis_and_angle(cft.zaxis, math.radians(90), point=cft.point)
        cft = cft.transformed(R)
       
    add = math.sin(i/5)*0.015
    #print(add)
    
    o1 = cft.point + cft.yaxis*(0.075+add)
    o2 = cft.point - cft.yaxis*(0.075+add)
    
    f1 = Frame(o1, cft.xaxis, cft.yaxis)
    f2 = Frame(o2, cft.xaxis, cft.yaxis)
    
    planes.append(draw_frame(f1))
    planes.append(draw_frame(f2))
    
    T1 = Transformation.from_frame_to_frame(Frame.worldXY(), f1)
    T2 = Transformation.from_frame_to_frame(Frame.worldXY(), f2)
    
    #T1 = Translation([10,2,3])
    
    # create new instances of the element
    new_elem_1 = elem.transformed(T1) #returns a deep copy of the initial element at the new location of the frame
    new_elem_2 = elem.transformed(T2)

    assembly.add_element(new_elem_1)
    assembly.add_element(new_elem_2)


if transform:
    T = Transformation.from_frame_to_frame(cf, Frame(ptransf.Origin, ptransf.XAxis, ptransf.YAxis))
    assembly.transform(T)




