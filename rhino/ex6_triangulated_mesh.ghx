                    <item name="ReferenceCount" type_name="gh_int32" type_code="3">0</item>
                    <item name="ScriptSource" type_name="gh_string" type_code="10">
    Component.Params.Input(0).Name = "Mesh"
    Component.Params.Input(0).Description = "Open or closed mesh"
    Component.Params.Input(1).Name = "Both Sides"
    Component.Params.Input(1).Description = "If true the mesh is offset in both directions along the vertex normals"
    Component.Params.Input(2).Name = "Edges"
    Component.Params.Input(2).Description = "If true the mesh will contain faces on the edges between the two offset mesh faces"
    Component.Params.Input(3).Name = "Closed Bottom"
    Component.Params.Input(3).Description = "If true the mesh will be closed on the origin face"
    Component.Params.Input(4).Name = "Closed Top"
    Component.Params.Input(4).Description = "If true the mesh will be closed on the offset face"
    Component.Params.Input(5).Name = "Vector [Vertex]"
    Component.Params.Input(5).Description = "An optional vector that sets the direction of extrusion, if empty, the new mesh will be offset along the vertex normals"
    Component.Params.Input(6).Name = "Distance [Vertex, Rh Units]"
    Component.Params.Input(6).Description = "A single number or list of numbers corresponding to each vertex of a mesh which sets the Rhino unit based distance that the new geometry is offset from each mesh face"

    Component.Params.Output(1).Name = "Mesh"
    Component.Params.Output(1).Description = "If successful, a valid mesh"

    If m IsNot Nothing Then

      Dim mf As Rhino.Geometry.Collections.MeshFaceList = Nothing
      Dim mv As Rhino.Geometry.Collections.MeshVertexList = Nothing
      Dim mtv As Rhino.Geometry.Collections.MeshTopologyVertexList = Nothing
      Dim mte As Rhino.Geometry.Collections.MeshTopologyEdgeList = Nothing
      Dim mn As Rhino.Geometry.Collections.MeshVertexNormalList = Nothing
      Dim mc As Rhino.Geometry.Collections.MeshVertexColorList = Nothing

      mf = m.Faces
      mv = m.Vertices
      mtv = m.TopologyVertices
      mte = m.TopologyEdges
      mn = m.Normals
      mc = m.VertexColors

      Dim clrB As Boolean = False
      If mc.count() &gt; 0 Then clrB = True

      Dim i,cv,cf,u(1),hd As Integer
      Dim blnV As Boolean
      Dim vec As vector3d
      Dim ova, ovb As New list(Of point3d)
      Dim vf As New list(Of Vector3f)
      Dim ofc As New list(Of MeshFace)
      If b Then hd = 2 Else hd = 1
      
      cv = mv.count()
      cf = mf.count()

      If (D.count() - 1) &lt; cv Then D = matchDbl(D, cv)

      If v.count() &gt; 0 Then
        blnV = True
        If (V.count() - 1) &lt; cv Then V = matchVec(V, cv)
      End If

      'Define Base Vertices
      For i = 0 To cv - 1 Step 1
        If blnV Then vec = V(i) Else vec = mn(i)
        vec.Unitize()
        vec *= (D(i) / hd)

        ovb.add(New point3d(mv(i)) + vec)
        If Not b Then vec *= 0
        vec.Reverse()
        ova.add(New point3d(mv(i)) + vec)
      Next

      If (c0 Or (((Not c0) And (Not e)) And c1)) Then ofc = mf.ToList()

      If ((e or c0) And c1) Then
        For i = 0 To cf - 1 Step 1
          If mf(i).isquad() Then
            ofc.add(New meshface(mf(i).A + cv, mf(i).B + cv, mf(i).C + cv, mf(i).D + cv))
          Else
            ofc.add(New meshface(mf(i).A + cv, mf(i).B + cv, mf(i).C + cv))
          End If
        Next
      End If

      If e Then
        For i = 0 To mte.count() - 1 Step 1

          u(0) = mtv.MeshVertexIndices(mte.GetTopologyVertices(i).I)(0)
          u(1) = mtv.MeshVertexIndices(mte.GetTopologyVertices(i).J)(0)

          If uBound(mte.GetConnectedFaces(i)) &lt; 1 Then
            ofc.add(New meshface(u(0), u(1), u(1) + cv, u(0) + cv))
          End If
        Next
      End If

      Dim mo As New mesh

      If (c0 Or e) Then
        mo.Vertices.AddVertices(ova.toarray())
        If clrB Then mo.VertexColors.AppendColors(mc.ToArray())
      End If
      If (c1 Or e) Then
        mo.Vertices.AddVertices(ovb.toarray())
        If clrB Then mo.VertexColors.AppendColors(mc.ToArray())
      End If

      mo.Faces.AddFaces(ofc)

      A = mo
    End If</item>
