                    <item name="CodeInput" type_name="gh_string" type_code="10">import logging
from System import TimeSpan
from System.Threading import ManualResetEventSlim

LOGGER = logging.getLogger('roslibpy')

if connect:
    if not ros_client.is_connected:
        LOGGER.debug('Connecting...')
        waitLock = ManualResetEventSlim(False)
        ros_client.on_ready(lambda: waitLock.Set())
        ros_client.connect()

        if not waitLock.Wait(TimeSpan.FromSeconds(5)):
            raise Exception('Unable to connect to ROS')
        LOGGER.debug('Reconnected')

if disconnect:
    if ros_client.is_connected:
        ros_client.terminate()
        LOGGER.debug('Terminated')
        
       
if (ros_client.is_connected):
    print('connected')
else:
    print('disconnected')
