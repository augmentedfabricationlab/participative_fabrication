                  <items count="12">
                    <item name="CodeInput" type_name="gh_string" type_code="10">import rhinoscriptsyntax as rs
import Rhino.Geometry as rg
from compas.geometry import cross_vectors
from compas.geometry import Vector, Frame
from compas_ghpython.utilities import draw_frame

planes = []

for mesh in mfaces:
    # vertex 0 and 1
    vertex1 = rs.MeshVertices(mesh)[0]
    vertex2 = rs.MeshVertices(mesh)[1]
    
    # set the origin of the plane in the middle of vertex 0 and 1
    origin = (rs.MeshVertices(mesh)[0]+rs.MeshVertices(mesh)[1])/2.0
    
    # compute x_axis vector
    x_axis = Vector(vertex1[0],vertex1[1], vertex1[2]) - Vector(vertex2[0],vertex2[1], vertex2[2])
    
    # extract face normal
    face_normal = Vector(rs.MeshFaceNormals(mesh)[0][0], rs.MeshFaceNormals(mesh)[0][1], rs.MeshFaceNormals(mesh)[0][2])
    
    # compute y_axis as cross product of x_axis and the face normal
    y_axis = cross_vectors(x_axis, face_normal)
    
    # append planes to a list
    planes.append(draw_frame(Frame(origin, x_axis, y_axis)))
</item>
