                    <item name="AdditionalSource" type_name="gh_string" type_code="10">  Private Function matchDbl(ByVal a As list(Of Double), ByVal t As Integer)
    Dim i,k As Integer
    k = a.count()
    For i = 0 To ((t + 1) - k) - 1 Step 1
      a.add(a.item(k - 1))
    Next
    Return a
  End Function

  Private Function matchVec(ByVal a As list(Of vector3d), ByVal t As Integer)
    Dim i,k As Integer
    k = a.count()
    For i = 0 To ((t + 1) - k) - 1 Step 1
      a.add(a.item(k - 1))
    Next
    Return a
  End Function</item>
