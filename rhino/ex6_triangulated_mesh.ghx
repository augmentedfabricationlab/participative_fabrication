import roslibpy
from roslibpy import Topic
from roslibpy import ServiceRequest
from roslibpy import Service
from roslibpy import Message
from scriptcontext import sticky


LOGGER = logging.getLogger('roslibpy') 


def ros_msg_add_building_steps():
    ros_msg = {}
    building_steps = []
    for key, element, data in assembly.elements(data=True):
        x,y,z,qw,qx,qy,qz = element.tool_frame_pose_quaternion
        if key &lt; num_built_elems:
            is_already_built = True  #data['is_placed']
        else:
            is_already_built = False
        building_steps.append({'pose': {'orientation': {'z': qz, 'y': qy, 'x': qx, 'w': qw}, 'position': {'z': z, 'y': y, 'x': x}},'object_type': object_type, 'is_already_built': is_already_built})
    ros_msg['building_steps'] = building_steps
    return ros_msg


if send:
    print("send")
    if ros_client:
            message = ros_msg_add_building_steps() #geometry_msgs/PoseStamped[] 
            #message = handle_add_two_bricks()[0] #geometry_msgs/PoseStamped[] 
            #print (message)                
            service_name = '/incon/add_building_steps'
            service_type = '/incon_msgs/add_building_steps'
            service = roslibpy.Service(ros_client, service_name, service_type)
            request = ServiceRequest(message)
            result = service.call(request, timeout=1) #sending of message
            #print(result)
            sticky['result_add_objects'] = result
            
            
            if result:
