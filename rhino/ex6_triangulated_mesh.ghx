                    <item name="CodeInput" type_name="gh_string" type_code="10">import time
import logging
import scriptcontext as sc

from roslibpy import Ros
from System import TimeSpan
from System.Threading import ManualResetEventSlim

LOGGER = logging.getLogger('roslibpy')

guid = str(ghenv.Component.InstanceGuid)
ros_client_key = "rosclient_" + guid

previous_client = sc.sticky.get(ros_client_key, None)
if previous_client:
    previous_client.terminate()

if connection == 0: #local
    ros_client = Ros('127.0.0.1', 9090) #local, own laptop
elif connection == 1: # linux computer
    ros_client = Ros('192.168.2.106', 9090) #mabi computer - lan
sc.sticky[ros_client_key] = ros_client

waitLock = ManualResetEventSlim(False) 
ros_client.on_ready(lambda: waitLock.Set()) 

if not waitLock.Wait(TimeSpan.FromSeconds(5)):
    raise Exception('Unable to connect to ROS')

LOGGER.info('ROS connection opened')

